{
  "name": "api-login-rest",
  "version": "1.0.0",
  "description": "REST API for login with service/controller/repository patterns",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "NODE_ENV=test npx mocha 'tests/**/*.test.js' --recursive --timeout 5000 --exit --reporter spec",
    "test:unit": "NODE_ENV=test mocha tests/unit/**/*.test.js --recursive --timeout 5000 --exit --reporter spec",
    "test:integration": "NODE_ENV=test mocha tests/integration/**/*.test.js --recursive --timeout 5000 --exit --reporter spec",
    "test:external": "NODE_ENV=test mocha tests/external/**/*.test.js --recursive --timeout 5000 --exit --reporter spec",
    "test:watch": "NODE_ENV=test mocha tests/**/*.test.js --recursive --timeout 5000 --watch --reporter spec",
    "test:coverage": "NODE_ENV=test nyc mocha tests/**/*.test.js --recursive --timeout 5000 --exit --reporter spec"
  },
  "keywords": [
    "api",
    "rest",
    "login",
    "javascript",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "chai": "^4.3.8",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "sinon": "^15.2.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
